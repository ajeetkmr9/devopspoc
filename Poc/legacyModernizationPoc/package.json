{
  "name": "glsustainableextension",
  "displayName": "GLSustainableExtension",
  "description": "converts code to another language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:extension.showWebView",
    "onView:package-explorer"
  ],
  "commands": [
    {
      "command": "extension.showWebView",
      "title": "Show WebView",
      "category": "WebView"
    }
  ],
  "contributes": {
    "viewsContainers": {
        "activitybar": [
            {
                "id": "package-explorer",
                "title": "Sustainable code",
                "icon": "media/dep.svg"
            }
        ]
    },
    "views": {
        "package-explorer": [
            {
                "id": "nodeDependencies",
                "name": "Hitachi",
                "icon": "media/dep.svg",
                "contextualTitle": "GL"
            },
            {
                "id": "yourTreeViewId",
                "name": "Your Tree View",
                "contextualTitle": "GL Tree View"
            },
            {
                "id": "yourWebViewId",
                "name": "Your WebView",
                "contextualTitle": "GL WebView"
            },
            {
              "id": "yourWebViewId",
              "name": "Your WebView",
              "contextualTitle": "GL WebView"
            }
        ],
        "webviewViews": [
            {
                "id": "yourWebViewId",
                "name": "Your WebView",
                "webviewView": "./out/yourWebView.js"
            }
        ]
    },
    "commands": [
        {
            "command": "extension.showTreeView",
            "title": "Show Tree View"
        },
        {
            "command": "extension.showWebView",
            "title": "Show WebView"
        }
    ]
},
  "webview": {
    "retainContextWhenHidden": true
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint \"src/**/*.ts\""
  },
  "devDependencies": {
    "@types/ftp": "^0.3.33",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^16.18.34",
    "@types/rimraf": "^2.0.2",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.26.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "ftp": "^0.3.10",
    "jsonc-parser": "^0.4.2",
    "minimist": "^1.2.6",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.2"
  }
}

