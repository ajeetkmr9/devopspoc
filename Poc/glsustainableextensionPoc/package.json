{
  "name": "vscode-extension-sustainable-code",
  "displayName": "VS Code Extension - sustainable code",
  "description": "sustainable code extension converts code from one language to other language",
  "version": "1.0.0",
  "publisher": "vscode-extension-sustainable-code",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscodeSidebar.openview",
    "onView:vscodeSidebar.openview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "custom-activitybar",
          "title": "VSCode Extension",
          "icon": "assets/logo_bito.svg"
        }
      ]
    },
    "views": {
      "custom-activitybar": [
        {
          "type": "webview",
          "id": "vscodeSidebar.openview",
          "name": "View",
          "contextualTitle": "View"
        }
      ]
    },
    "commands": [
      {
        "command": "vscodeSidebar.openview",
        "title": "Sidebar View"
      },
      {
        "command": "vscodeSidebar.menu.view",
        "category": "vscode-extension-sidebar-html",
        "title": "Sample WebView in VS Code Sidebar",
        "icon": "$(clear-all)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "vscodeSidebar.menu.view",
          "group": "navigation",
          "when": "view == vscodeSidebar.openview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/glob": "^7.1.4",
    "@types/json-server": "^0.14.7",
    "@types/mocha": "^9.0.0",
    "@types/node": "^20.10.5",
    "@types/typescript": "^2.0.0",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.56.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.3",
    "express": "^4.18.2",
    "json-server": "^0.17.4",
    "eslint": "8.56.0",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "^4.33.0"
  }
}
